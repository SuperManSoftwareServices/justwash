    <section class="sign-up section-padding text-center" id="booking">
        <div class="content-section" id="booking">
            <div class="container">
                <div class="row">
                    <div class="heading-section col-lg-12" style=" margin-top : -25px">
                        <h3>Book Now</h3>
                        <form name="form" class="signup-form" action="booknow" method="POST" role="form" style=" margin-top : auto ; ">
                            <div class="form-input-group">
                                <i class="fa fa-male"></i><input type="text" id="uname" name="uname" pattern="[A-Za-z]+" title="Please Enter Your Name" class="" placeholder="Enter your Name" required >
                            </div>
                            <div class="form-input-group">
                               <i class="fa fa-mobile-phone"></i><input type="text" id="mobNum" pattern="[7-9]{1}[0-9]{9}" title="example 9898987898"name="mobNum"  class="" placeholder="Enter your Mobile Number *" required>
                            </div>
                            <div class="form-input-group">
                                <i class="fa fa-map-marker"></i><input type="text" id="location" name="location" pattern="[A-Za-z]+" title="Area for pickup" class="" placeholder="Area of Pick up *" required>
                            </div>
                            <div class="form-input-group">
                               <i class="fa fa-location-arrow"></i><input type="text" id="location" name="location"  title="Please Provider pick up address" class="" placeholder="Pickup Address *" required>
                            </div>
							<div class="form-input-group">
                                <i class="fa fa-map-marker"></i><input type="text" id="location" name="location" pattern="[A-Za-z]+" title="Enter Your Landmark" class="" placeholder="Landmark *" required>
                            </div>
                            <div class="form-input-group">
                                <i class="fa fa-calendar"></i><input type="date" id="location" name="location" title="Enter The Date" class="" placeholder="Pickup Date *" required>
                            </div>
							<div class="form-input-group" >
                                <i class="fa fa-clock-o"></i><input type="text" id="location" name="location" title="Enter The Pickup Time" class="" placeholder="Enter The Pickup Time *" required>
                            </div>
							<button type="submit" class="btn-fill sign-up-btn">BookNow</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

